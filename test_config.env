# Test configuration for faster sync agent timing
# Source this file before running tests: source test_config.env

export SCOPE_SYNC_INTERVAL=5          # sync scopes every 5 seconds instead of 60
export DELETE_HDF5_CACHE_INTERVAL=10  # check cache every 10 seconds instead of 60
export USER_CHECK_INTERVAL=1          # check user login every 1 second instead of 5
export CONNECTION_ERROR_DELAY=5       # wait 5 seconds on connection error instead of 60
export IDLE_DELAY=0.1                 # 0.1 second delay when idling instead of 1 